{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "wwwroot/dist/",
    "target": "es2020",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
